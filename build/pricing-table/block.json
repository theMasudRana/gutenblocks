{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "gutenblocks/pricing-table",
  "version": "1.0.0",
  "title": "GTB: Pricing Table",
  "category": "design",
  "icon": "tablet",
  "keywords": [
    "pricing",
    "table",
    "gutenblocks"
  ],
  "description": "Pricing Table block for Gutenblocks",
  "example": {},
  "supports": {
    "html": false,
    "color": {
      "background": true,
      "text": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    },
    "border": {
      "radius": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "default": "Pricing Title"
    },
    "subtitle": {
      "type": "string",
      "default": "Pricing subtitle"
    },
    "currency": {
      "type": "string",
      "default": "$"
    },
    "price": {
      "type": "string",
      "default": "99"
    },
    "onSale": {
      "type": "boolean",
      "default": false
    },
    "salePrice": {
      "type": "string",
      "default": "49"
    },
    "priceSeparator": {
      "type": "string",
      "default": "/"
    },
    "pricePeriod": {
      "type": "string",
      "default": "month"
    },
    "features": {
      "type": "array",
      "default": [
        {
          "text": "Feature 1"
        }
      ]
    },
    "buttonText": {
      "type": "string",
      "default": "Buy Now"
    },
    "buttonUrl": {
      "type": "string",
      "default": "#"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "style": {
      "type": "object",
      "default": {
        "color": {
          "background": "#f9f9f9",
          "text": "#333"
        },
        "spacing": {
          "padding": {
            "top": "20px",
            "right": "20px",
            "bottom": "20px",
            "left": "20px"
          }
        },
        "border": {
          "radius": "4px"
        }
      }
    }
  },
  "textdomain": "gutenblocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}