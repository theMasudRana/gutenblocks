{
	"name": "masud/gutenblocks",
	"description": "A WordPress plugin for Gutenberg blocks",
	"type": "wordpress-plugin",
	"authors": [
		{
			"name": "Masud Rana",
			"email": "mr.masudrana00@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "^3.0",
		"phpcompatibility/phpcompatibility-wp": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Gutenblocks\\": "includes/"
		}
	}
}
